                  <form>
                     <orth xml:lang="es">de nuevo</orth>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Sara nintivi tuki ini nto'o lochi ka .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">Then he went back inside the buzzard's basket.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Y entró en el tenate del zopilote de nuevo.</quote>
                  </cit>
               </cit>
            </sense>
