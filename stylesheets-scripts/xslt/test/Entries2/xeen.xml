   <text>
      <body>
         <entry>
            <form type="lemma">
               <orth xml:lang="mix">xeen</orth>
            </form>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Cha viko ka kunkukui nuu suku xeen ka vikó .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">And the party will be really high up in the clouds.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Y la fiesta va a ser a muy gran altura en las nubes.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Sara ntutsi xeen inia ra ntavi .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">Then his feelings were hurt and he flew away.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Pues sus sentimientos eran heridos.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Viko ka va'a xeen ntsio , ña ntsio ntsatsia , ntsio yaa .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">The party was really nice, they ate, there was music.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">La fiesta fue muy bien, comieron, había música.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">— Amigu , kunu'u yu vari kuni xeen kusu yu .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">Friend, I'm going to leave because I'm very tired.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Amigo, voy irme por qué estoy muy cansado.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Ntavi ta sa'va , nikana kavi mancha nuu vikó ni'i xeen niyai nuu ñu'u .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">The frog flew a lot, and it fell down rapidly from the clouds and fell to the ground.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">La rana voló mucho, cayó todo el camino de las nubes y cayó a</quote>
                  </cit>
               </cit>
            </sense>
