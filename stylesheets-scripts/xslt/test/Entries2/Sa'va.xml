                     <w xml:id="d1e7">Sa'va</w>
                     <w xml:id="d1e9">tsi</w>
                     <w xml:id="d1e11">Lochi</w>
                  </seg>
               </title>
               <title xml:lang="es">
                  <seg xml:id="L155-00-02">
                     <w xml:id="d1e15">La</w>
                     <w xml:id="d1e17">rana</w>
                     <w xml:id="d1e19">y</w>
                     <w xml:id="d1e21">el</w>
