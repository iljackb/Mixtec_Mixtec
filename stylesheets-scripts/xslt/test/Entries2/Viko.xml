   <text>
      <body>
         <entry>
            <form type="lemma">
               <orth xml:lang="mix">viko</orth>
            </form>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">In kii , amigu lochi nikani sa'va ka ku'in in viko tanta'a .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">One day the vulture friend invited the frog to go to a wedding party.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Un día el amigo zopilote le invitó la rana a ir a una fiesta de boda.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en">party</orth>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es">fiesta</orth>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Cha viko ka kunkukui nuu suku xeen ka vikó .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">And the party will be really high up in the clouds.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Y la fiesta va a ser a muy gran altura en las nubes.</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en">party</orth>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es">fiesta</orth>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Inka tuku kii , lochi ka nikana tuki sa'va ka inka tuku viko nuu vikó .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">Later that day, the buzzard again invited the frog to another party in the clouds.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Más tarde en el día, el zopilote le invitó de nuevo la rana a una otra fiesta en las nubes. </quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en"/>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Ntsika koyi nuu viko ntsain yaa .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">They were standing (in the clouds | at the party) dancing.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Estaban (en las nubes | a la fiesta) bailando.</quote>
                  </cit>
               </cit>
            </sense>
