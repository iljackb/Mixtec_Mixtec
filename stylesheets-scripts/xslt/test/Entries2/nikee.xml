   <text>
      <body>
         <entry>
            <form type="lemma">
               <orth xml:lang="mix">nikee</orth>
            </form>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en">came out</orth>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es">salió</orth>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Ta ntsai vikó ka , sara nikee sa'va ka ini nto'o sara nikachia :</quote>
                  <cit type="translation">
                     <quote xml:lang="en">When he got to the clouds, the frog came out from the basket and said:</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Cuando llegó en las nubes, la rana salió del tenate y dijó:</quote>
                  </cit>
               </cit>
            </sense>
            <sense>
               <cit type="translation">
                  <form>
                     <orth xml:lang="en">got out</orth>
                  </form>
               </cit>
               <cit type="translation">
                  <form>
                     <orth xml:lang="es">salió</orth>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Tsio saan , sa'va ka nikee tuki ini nto'o .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">And so it happened, the frog got out again of the basket again.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Así sucedío, la rana salió de nuevo del tenate</quote>
                  </cit>
               </cit>
            </sense>
