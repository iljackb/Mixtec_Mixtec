                  <form>
                     <orth xml:lang="es"/>
                  </form>
               </cit>
               <cit type="example">
                  <quote xml:lang="mix">Cha ntsiko so nui vari ntsain nchu'i yaa .</quote>
                  <cit type="translation">
                     <quote xml:lang="en">And he was walking around dazed because he was dancing a lot.</quote>
                  </cit>
                  <cit type="translation">
                     <quote xml:lang="es">Y caminaba aturido porque he bailado mucho.</quote>
                  </cit>
               </cit>
