                     <span target="#d1e198" xml:lang="en">let's go</span>
                     <span target="#d1e198" xml:lang="es">vamos</span>
                     
                     <span target="#d1e200 #d1e202" xml:lang="en">to the corn field</span>
                     <span target="#d1e200 #d1e202" xml:lang="es">al maízal</span>
                     
                     <span target="#d1e200 #d1e202 #d1e204" xml:lang="en">to the people's corn field</span>
                     <span target="#d1e200 #d1e202 #d1e204" xml:lang="es" cert="medium">al maízal de la gente</span>
                     
