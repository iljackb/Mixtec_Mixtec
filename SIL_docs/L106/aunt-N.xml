<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>xixi 'aunt' paradigm</title>
         </titleStmt>
         <publicationStmt>
            <p>Publication Information</p>
         </publicationStmt>
         <sourceDesc>
            <p>Information about the source</p>
         </sourceDesc>
      </fileDesc>
  </teiHeader>
  <text>
      <body>
         <entry xml:lang="mix" xml:id="aunt-N-paradigm-MIX">
            <form type="lemma">
               <orth xml:lang="mix">xixi</orth>
               <pron notation="ipa" xml:lang="mix" source="#Pike-Ibach-MIX-1978">xi˧xi˩</pron>
            </form>
            <gramGrp>
               <pos>noun</pos>
            </gramGrp>
            
            <form type="paradigm" subtype="possessive">
               
               <gramGrp>
                  <gram norm="possessive">poss</gram>
               </gramGrp>     
               
               <!-- 1S -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <pron xml:lang="mix" notation="ipa"></pron>
                  
                  <gramGrp>
                     <per>1</per>
                     <number norm="singular">sg</number>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 2S.INF -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>2</per>
                     <number norm="singular">sg</number>
                     <gram type="register" norm="informal">inf</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 2S.FORM -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>2</per>
                     <number norm="singular">sg</number>
                     <gram type="register" norm="formal">form</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>            
               
               <!-- 3S.ANML  -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>3</per>
                     <number norm="singular">sg</number>
                     <gen cert="medium">animl</gen>
                     <gram type="register" norm="informal">inf</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 3S.CHILD  -->
               <form type="inflected">
                  <orth xml:lang="mix">xixi tsi</orth>
                  <pron notation="ipa" xml:lang="mix" source="#Pike-Ibach-MIX-1978">xi˧xi˩ tsi˧</pron>
                  <gramGrp>
                     <per>3</per>
                     <number norm="singular">sg</number>
                     <gen cert="medium">child</gen>
                     <gram type="register" norm="informal">inf</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 3S.FORM.M -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>3</per>
                     <number norm="singular">sg</number>
                     <gen norm="masculine">masc</gen>
                     <gram type="register" norm="formal">form</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 3S.FORM.F  -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>3</per>
                     <number norm="singular">sg</number>
                     <gen norm="feminine">fem</gen>
                     <gram type="register" norm="formal">form</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!--  1PL.EXCL -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>1</per>
                     <number norm="plural">pl</number>
                     <gram type="inclusivity" norm="exclusive">excl</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 1PL.INCL -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>1</per>
                     <number norm="plural">pl</number>
                     <gram type="inclusivity" norm="inclusive">incl</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 2PL.INF -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>2</per>
                     <number norm="plural">pl</number>
                     <gram type="register" norm="informal">inf</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>
               
               <!-- 2PL.FORM -->
               <form type="inflected">
                  <orth xml:lang="mix"></orth>
                  <gramGrp>
                     <per>2</per>
                     <number norm="plural">pl</number>
                     <gram type="register" norm="formal">form</gram>
                  </gramGrp>
                  <gloss xml:lang="en"></gloss>
                  <gloss xml:lang="es"></gloss>
               </form>            
            </form>
            
            <!--  3PL.INF-->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gram type="register" norm="informal">inf</gram>
               </gramGrp>
            </form>
            
            <!-- 3PL.FORM.M -->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gen norm="masculine">masc</gen>
                  <gram type="register" norm="formal">form</gram>
               </gramGrp>
            </form>
            
            <!-- 3PL.FORM.F-->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gen norm="feminine">fem</gen>
                  <gram type="register" norm="formal">form</gram>
               </gramGrp>
            </form>
            
            <!-- 3PL.FORM.ALL.HUM -->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gram type="inclusivity" norm="inclusive">incl</gram>
                  <gram type="register" norm="formal">form</gram>
                  <!-- human -->
               </gramGrp>
               <note cert="medium">Alternatively the pronoun could be <oRef>kuena</oRef>.</note>
            </form>
            
            <!-- 3PL.ANML  -->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gen cert="medium">animl</gen>
                  <gram type="register" norm="informal">inf</gram>
               </gramGrp>     
            </form>
            
            <!-- 3PL.CHILD  -->
            <form type="inflected">
               <orth xml:lang="mix"></orth>
               <gramGrp>
                  <per>3</per>
                  <number norm="plural">pl</number>
                  <gen cert="medium">child</gen>
                  <gram type="register" norm="informal">inf</gram>
               </gramGrp>     
            </form>
            
            <sense>
               <cit type="translation">
                  <orth xml:lang="en">aunt</orth>
               </cit>
               <cit type="translation">
                  <orth xml:lang="es">tía</orth>
               </cit>
            </sense>
         </entry>
      </body>
  </text>
</TEI>
