                  <w xml:id="d1e758">sketi</w>
                  <w xml:id="d1e760">sketi</w>
                  <pc>.</pc>
               </seg>
               <spanGrp type="translation">
                  <span target="#L150-06-03" xml:lang="en">Then he ran and ran</span>
                  <span target="#L150-06-03" xml:lang="es"></span>
                  
                  <span target="#" xml:lang="en"></span>
                  <span target="#" xml:lang="es"></span>
