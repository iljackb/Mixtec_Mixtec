                  <w xml:id="d1e667">inia</w><!-- -a -->
                  <pc>:</pc>
               </seg>
               <spanGrp type="translation">
                  <span target="#L150-05-01" xml:lang="en">And he though to himself:</span>
                  <span target="#L150-05-01" xml:lang="es"></span>
                  
                  <span target="#" xml:lang="en"></span>
                  <span target="#" xml:lang="es"></span>
               </spanGrp>
               <seg xml:id="L150-05-02" type="S">
                  <pc>"</pc>
                  <w xml:id="d1e673">Vichi</w><!-- now -->
                  <w xml:id="d1e675">ra</w>
                  <w xml:id="d1e677">nasketa</w><!-- I must run  -->
                  <w xml:id="d1e679">ko'on</w><!-- go -->
                  <w xml:id="d1e681">takua</w><!-- so that -->
                  <w xml:id="d1e683">nantuva'a</w><!-- feel/get better -->
                  <w xml:id="d1e685">titso</w><!-- stomach (why -o)? -->
                  <pc>"</pc>
