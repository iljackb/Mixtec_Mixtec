                  <w xml:id="d1e769">ntakunchi'i</w><!-- open (his) eyes -->
                  <w xml:id="d1e771">ra</w>
                  <w xml:id="d1e773">ntsinia</w><!-- he saw -->
                  <w xml:id="d1e775">ña</w><!-- that -->
                  <w xml:id="d1e777">likui</w><!-- the frog -->
                  <w xml:id="d1e779">ka</w>
                  <w xml:id="d1e781">kuu</w><!-- be  (how to translate usage?) -->
                  <w xml:id="d1e783">ña</w>
                  <w xml:id="d1e785">nikanti</w><!--  -->
                  <w xml:id="d1e788">titsi</w>
                  <pc>.</pc>
               </seg>
               <spanGrp type="translation">
                  <span target="#L150-06-04" xml:lang="en">And when he opened his eyes he saw that the frog's stomach had exploded.</span>
                  <span target="#L150-06-04" xml:lang="es"></span>
                  
                  <span target="#" xml:lang="en"></span>
                  <span target="#" xml:lang="es"></span>
