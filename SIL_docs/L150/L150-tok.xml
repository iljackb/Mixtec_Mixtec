                  <w xml:id="d1e749">nikini</w><!-- (TS) should be "niki'ni") -->
                  <w xml:id="d1e751">na</w>
                  <pc>.</pc>
               </seg>
               <spanGrp type="translation">
                  <span target="#L150-06-02" xml:lang="en">We've been shot</span>
                  <span target="#L150-06-02" xml:lang="es"></span>
                  
                  <span target="#" xml:lang="en"></span>
                  <span target="#" xml:lang="es"></span>
               </spanGrp>
               <seg xml:id="L150-06-03" type="S">
