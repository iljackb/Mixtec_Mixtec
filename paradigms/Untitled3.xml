               </cit>
               <cit type="translation">
                  <orth xml:lang="es">beber</orth>
               </cit>
            </sense>
            <form type="paradigm" cert="low">
               <!-- PRESENT/INCOMPLETIVE -->
               <gramGrp>
                  <gram type="aspect" norm="imperfective">impv</gram>                   <mood>realis</mood>
                  <mood>realis</mood>
               </gramGrp>
               
            <!-- 1S -->
               <form type="inflected">
                  <gramGrp>
                     <per>1</per>
                     <number norm="singular">sg</number>
                  </gramGrp>
                  <orth xml:lang="mix">tsí'i</orth>
                  <pron xml:lang="mix" notation="ipa"/><!-- 1sg is important to get pron because the tone affects the inflection -->
