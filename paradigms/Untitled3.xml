         <entry xml:lang="mix" xml:id="drink-V-paradigm-MIX">
            <form type="lemma">
               <orth xml:lang="mix">ko'o</orth>
            </form>
            <gramGrp>
               <pos>verb</pos>
            </gramGrp>
            <sense>
               <cit type="translation">
