      <body>
         <entry xml:id="body-face">
            
            <form type="lemma">
               <orth>nuu</orth>               
               
               <pron notation="ipa">nũ̀ṹ</pron>
               <pron notation="zsampa">n u:_n_F</pron>
               
               <gramGrp>
                  <pos>noun</pos>
               </gramGrp>
            </form>
            
            <sense xml:id="face-PRIME" corresp="http://dbpedia.org/page/Category:Face"><!-- #PRIME @ana="#FACE" ?-->
               
               <usg type="domain" corresp="http://dbpedia.org/ontology/AnatomicalStructure">AnatomicalStructure</usg>
               <!-- (meronymy) isA:AnatomicalStructure -->
               
               <!-- add 'frontal configuration' unless it is already in dbpedia entry for 'face' -->
               
               <usg type="semanticRelation"></usg>

               <cit type="translation" xml:lang="en">
                  <quote>face</quote>
               </cit>
               <cit type="translation" xml:lang="es">
                  <quote>cara</quote>
               </cit>
            </sense>
            
            <sense ana="#OBLQ #LM">           
               <form type="inflected">
                  <orth>nuu̠</orth>
               </form>
               <gramGrp>
                  <pos></pos>
                  <gram></gram>
                  <gram></gram>
               </gramGrp>
               <usg type="semanticRole" ana="#LM-PERS">LM</usg>
              
               <def xml:lang="en">pronoun referring to a whole person</def>
               
               <!-- discourse? -->
               <cit type="example" source="#file-id.xml">
                  <quote>ntu'u saa ka nuu̠</quote>
                  
                  <cit type="translation" xml:lang="en">
                     <quote>The bird is sitting in front of me</quote>
                  </cit>
               </cit>
               
               <etym type="derivation">
                  
                  <cit type="etymon">
                     <oRef>nuu</oRef>
                     <def xml:lang="en">face</def>
                     <def xml:lang="en">face</def>
                  </cit>
                  
                  <etym type="metonymy" subtype="partForWhole"><!-- actually is "derrivation" & "metonymy" -->
                     <!-- possessive body part (individual) > individual  -->
                     <!-- posessorRaising -->
                     <desc></desc>
                     <usg></usg>
                     
                     
                     
                  </etym>
               </etym>
               
               
            </sense>
            
         </entry>
         
         <entry>
            <form type="lemma">
               <orth>nu</orth>
               <!-- in extended sense pronunciation varies more, thus links to files are desirable-->
               <pron source="#filea.wav">nũ̀ũ</pron>
               <pron>nũ̀ũ</pron>
               <!--  -->
               <pron>nũ̀</pron>
               <pron>nũ̀</pron>
               <pron></pron>
            </form>
            
            <sense ana="#FrontPartOf #SpatialIndicator">
               <!-- FrontOf -->
              
               <gramGrp>
                  <pos></pos>
               </gramGrp>
               
               <usg type="colloc" ana="#LM">LM</usg>
               <usg type="domain" ana="#MERONYMY"></usg>
               <usg type="" ana="#SpatialIndicator"></usg>
              
              <cit type="example">
                 <quote>nuu ve'e</quote>
                 <cit type="translation" xml:lang="en">
                    <quote>the front of the house</quote>
                 </cit>
              </cit>
              
              
               <def xml:lang="en"></def>
               <def xml:lang="es"></def>
               <def xml:lang="mix"></def>

               <etym type='metonymy'>
                  
                  <etym type="metaphor">
                     
                  </etym>
                  
               </etym>
               
            </sense>
            
            <sense>
               <!-- inFrontOf -->
               <def></def>
               
               <gramGrp>
                  <pos></pos>
               </gramGrp>
               
            </sense>
