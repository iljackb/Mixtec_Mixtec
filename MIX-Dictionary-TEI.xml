      <fileDesc>
         <titleStmt>
            <title xml:lang="en">Mixtepec-Mixtec Digital Dictionary</title>
            <title xml:lang="es">Diccionário Digital del Misteco de Mixtepec</title>
            <!-- Add Mixtec version of this!! (need to make up term for dictionary!!) (word + book) is probably the best option" -->
