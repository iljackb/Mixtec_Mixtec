      <revisionDesc>
         <listChange>
            <change>
               <date>2017-08-18T14:21:16Z</date>
               <name>Jack Bowers</name>
                <!-- changed capital letters -->
                <!-- changed spelling of: 
                    "nchi" (where) to "nchii";
                    "Saana inu" (forget) to "sana inu" (as per conventions)
                   "nishi" (how) to "nixi" (as per conventions)
                   "su luu" (child/baby) to "sulu" (as per conventions)
                   "nuu" (sense: relational noun/preposition) to "nu" (as per conventions)
                   "xuun" to "xu'un" (as per conventions)
                   "sa va'a" to "sava'a" (as per conventions)
                   "ixii" to "ixi" (as per conventions)
                   "sa'ana" to "sa'a na"
                   "Iin" (one) tp "in" (as per conventions)
                    In sentence: Nchi ku(u) (ñ)o'o? - "what is this?"
                      > "ku" to "kuu"
                      > "no'o" to "ño'o"
                      
                    "katcha" to "kachia" (in "nixi kunia kachia")
                    "katchu" to "kachu" (in "nixi kachu (perro))
                    
                    Added last example "¿Nixi kachi ni?" (formal version of previous) needs confirmation w/expert
                    
                -->
            </change>
         </listChange>
      </revisionDesc>
   </teiHeader>
   <text>
      <body>
         <p>
            <seg xml:id="d1e44" xml:lang="mix" type="MWE">
               <w xml:id="d1e45">ntsitu</w>
               <w xml:id="d1e47">titsi</w>
            </seg>:
                
                <hi rend="italic" xml:space="preserve">
                <spanGrp type="translation">
                <span target="#d1e44" xml:lang="en">to be full</span>
                <span target="#" xml:lang="es">estar lleno</span>
            </spanGrp></hi>
             
         </p>
         <p>
            <hi rend="italic">- 
                    <spanGrp type="translation"><!--  -->
                  <span target="#" xml:lang="en">my stomach is full</span>
                  <span target="#" xml:lang="es"/>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e68" xml:lang="mix">
               <w xml:id="d1e69" cert="medium">kunau'u</w><!-- SIL spells it "kuna'a" -->
            </seg>
            <hi rend="italic">: 
                    <spanGrp type="translation">
                        <span target="#d1e69" xml:lang="en">to remember</span>
                        <span target="#d1e69" xml:lang="es">recordar</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e78" xml:lang="mix" type="imperative">
               <w xml:id="d1e79">Kunau'u</w>
               <w xml:id="d1e81">nchii</w>
               <w xml:id="d1e83">kaku</w><!-- same as 'put down' -->
               <w xml:id="d1e85">xu'un</w>
            </seg>
            <hi rend="italic" xml:space="preserve"> -
                    <spanGrp type="translation">
                        <span target="#d1e78" xml:lang="en">Remember where you left the money</span>
                        <span target="#d1e78" xml:lang="es">Recuerdas dónde pusiste el dinero.</span>
                        
                        <span xml:lang="en" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                        
                        <span xml:lang="es" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                        
                        <span xml:lang="es" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                    </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e97" xml:lang="mix">
               <w xml:id="d1e98" cert="medium">tsituninu</w><!-- understand (glossform - tsitunini?) -->
            </seg>
            <hi rend="italic">: 
                <spanGrp type="translation">
                    <span target="#d1e98" xml:lang="en">to understand</span>
                    <span target="#d1e98" xml:lang="es">entender</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e107" xml:lang="mix" type="S">
               <w xml:id="d1e108">A</w>
               <w xml:id="d1e110">tsituninu</w>
               <w xml:id="d1e112">nixi</w>
               <w xml:id="d1e114">ka'an</w>
               <w xml:id="d1e116">yu</w>
            </seg> - 
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e107" xml:lang="en">Do you understand how I am speaking?</span>
                        <span target="#d1e107" xml:lang="en">Do you understand what I am saying?</span>
                        <span target="#" xml:lang="es"/>
                        
                        <span xml:lang="en" target="#d1e110">you understand</span>
                        <span xml:lang="en" target="#d1e110">entiendes</span>
                        
                        <span xml:lang="en" target="#d1e112">how</span>
                        <span xml:lang="en" target="#d1e112">cómo</span>
                        
                        <span xml:lang="en" target="#d1e114 #d1e116">I'm saying</span>
                         <span xml:lang="en" target="#d1e114 #d1e116">I'm speaking</span>
                        <span xml:lang="en" target="#d1e114 #d1e116">estoy diciendo</span>
                        <span xml:lang="en" target="#d1e114 #d1e116">estoy hablando</span>
                    </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e129" xml:lang="mix" type="MWE">
               <w xml:id="d1e130">sana</w>
               <w xml:id="d1e132">ini</w><!-- inflects on last vowel -->
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e129" xml:lang="en">forget</span>
                   <span target="#d1e129" xml:lang="es">olividar</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e141" xml:lang="mix" type="declarative">
               <w xml:id="d1e142">Sana</w>
               <w xml:id="d1e144">inu</w>
               <w xml:id="d1e146" cert="low">you</w><!-- is this right? -->
               <w xml:id="d1e148">nixi</w>
               <w xml:id="d1e150">nanu</w>
            </seg> - 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e141" xml:lang="en">I forgot what your name is.</span>
                   <span target="#d1e141" xml:lang="es">Olividé cómo te llamas.</span>
                   
                   <span xml:lang="en" target="#"></span>
                   <span xml:lang="es" target="#"></span>
                   
                   <span xml:lang="en" target="#"></span>
                   <span xml:lang="es" target="#"></span>
                   
                   <span xml:lang="en" target="#"></span>
                   <span xml:lang="es" target="#"></span>
                   
                   <span xml:lang="en" target="#d1e150">your name is</span>
                   <span xml:lang="es" target="#d1e150">te llamas</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e159" xml:lang="mix">
               <w xml:id="d1e160">ntane'e</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e160" xml:lang="en">to pick up</span><!-- pick up (something) -->
                   <span target="#d1e160" xml:lang="es">recoger</span><!-- recoger (algo) -->
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e172" xml:lang="mix" type="imperative">
               <w xml:id="d1e173">Ntane'e</w><!-- cmnd -->
               <w xml:id="d1e178">sa'mu</w>
            </seg> -       
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e172" xml:lang="en">Pick up your clothes.</span>
                   <span target="#d1e172" xml:lang="es">Recoge tus ropas.</span>
                   
                   <span xml:lang="en" target="#d1e173">pick up</span>
                   <span xml:lang="es" target="#d1e173">recoge</span>
                   
                   <span xml:lang="en" target="#d1e178">your clothes</span>
                   <span xml:lang="es" target="#d1e178">tus ropas</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e187" xml:lang="mix">
               <w xml:id="d1e188" cert="medium">kaki</w><!-- check infinitive!! -->
            </seg>:
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e188" xml:lang="en">to put down</span><!-- put down (something) -->
                   <span target="#d1e188" xml:lang="es">dejar</span>
                   <span target="#d1e188" xml:lang="es">colocar</span>
                   <span target="#d1e188" xml:lang="es">poner</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e198" xml:lang="mix" type="imperative">
               <w xml:id="d1e199">Kaki</w>
               <w xml:id="d1e201" cert="medium">sulu</w><!-- check spelling!! -->
               <w xml:id="d1e205" cert="low">ntsa</w><!-- that!? -->
               <w xml:id="d1e207">nu</w>
               <w xml:id="d1e209">sa'ma</w>
            </seg> - 
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e198" xml:lang="en">Put that baby on the clothes</span>
                        <span target="#d1e198" xml:lang="es">Ponga este bebé su las ropas.</span>
                        
                        <span xml:lang="en" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                        
                        <span xml:lang="en" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                        
                        <span xml:lang="en" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                        
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e222" xml:lang="mix" type="MWE">
               <w xml:id="d1e223">tina</w>
               <w xml:id="d1e225">luu</w>
            </seg>
            <hi rend="italic">: 
                <spanGrp type="translation">
                    <span target="#d1e222" xml:lang="en">puppy</span>
                    <span target="#d1e222" xml:lang="es">cachorro</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e234" xml:lang="mix" type="declarative">
               <w xml:id="d1e235">Tina</w>
               <w xml:id="d1e237">luu</w>
               <w xml:id="d1e239">sketa</w>
               <w xml:id="d1e241">ti</w>
               <w xml:id="d1e243">nu</w>
               <w xml:id="d1e245">yucha</w>
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e234" xml:lang="en" cert="medium">The puppy is running on the rivier.</span><!-- into the river? in the river? -->
                   <span target="#d1e234" xml:lang="es">El cachorro corre por el río.</span>
                   
                   <span xml:lang="en" target="#d1e235 #d1e237">puppy</span>
                   <span xml:lang="es" target="#d1e235 #d1e237">cachorro</span>
                   
                   <span xml:lang="en" target="#d1e239 #d1e241">it (animal) is running</span>
                   <span xml:lang="es" target="#d1e239 #d1e241">él (animal) está corriendo</span>
                   
                   <span xml:lang="en" target="#"></span>
                   <span xml:lang="es" target="#"></span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e254" xml:lang="mix">
               <w xml:id="d1e255">ntasama</w>
            </seg>:
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e255" xml:lang="en">to change</span><!-- change (something) -->
                   <span target="#d1e255" xml:lang="es">cambiar</span><!-- cambiar (algo) -->
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e264" xml:lang="mix" type="imperative">
               <w xml:id="d1e265">Ntsama</w>
               <w xml:id="d1e267">xu'un</w>
               <w xml:id="d1e269">yo'o</w>
            </seg> - 
                <hi rend="italic" xml:space="preserve">
                <spanGrp type="translation">
                    <span target="#d1e264" xml:lang="en">Change this money.</span>
                    <span target="#d1e264" xml:lang="es">Cambias este dinero.</span>
                    
                    <span xml:lang="en" target="#d1e265">change</span>
                    <span xml:lang="es" target="#d1e265">cambia</span><!-- cmnd -->
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e282" xml:lang="mix" type="MWE" cert="medium"><!-- check to see spelling conventions -->
               <w xml:id="d1e283">ni</w>
               <w xml:id="d1e285">saa</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">how much?</span>
                  <span target="#" xml:lang="es">quanto?</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e294" xml:lang="mix">
               <w xml:id="d1e295">Ni</w>
               <w xml:id="d1e297">saa</w>
               <w xml:id="d1e299" cert="low">ya'vi</w><!-- ?? cost -->
               <w xml:id="d1e304">vilu</w>
               <w xml:id="d1e306">ka</w>
            </seg> - 
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">How much is that cat?</span>
                  <span target="#" xml:lang="es">¿Quanto cuesta este gato?</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e315" xml:lang="mix">
               <w xml:id="d1e316">ntakuaan</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e316" xml:lang="en">to buy</span>
                   <span target="#d1e316" xml:lang="es">comprar</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e325" xml:lang="mix" type="imperative">
               <w xml:id="d1e326">Ntakuaan</w>
               <w xml:id="d1e328">sa'ma</w>
               <w xml:id="d1e330">yo'o</w>
            </seg> - 
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e325" xml:lang="en">Buy these clothes.</span>
                        <span target="#d1e325" xml:lang="es">Compra esta ropa.</span>
                        
                        <span xml:lang="en" target="#d1e326">buy</span>
                        <span xml:lang="es" target="#d1e326">compra</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e343" xml:lang="mix">
               <w xml:id="d1e344">cha'vi</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e344" xml:lang="en">to pay</span>
                   <span target="#d1e344" xml:lang="es">pagar</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e356" xml:lang="mix" type="imperative">
               <w xml:id="d1e357">Cha'vi</w>
               <w xml:id="d1e362" cert="low">tsaa</w><!-- this? the? "foot" (tsa'a)? -->
               <w xml:id="d1e364">ve'e</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e356" xml:lang="en">Pay for the house.</span>
                        <span target="#d1e356" xml:lang="es">Paga la casa.</span>
                        
                        <span xml:lang="en" target="#d1e357">pay</span>
                        <span xml:lang="es" target="#d1e357">paga</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e381" xml:lang="mix">
               <w xml:id="d1e382">xiko</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e382" xml:lang="en">to sell</span>
                   <span target="#d1e382" xml:lang="es">vender</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e391" xml:lang="mix" type="imperative">
               <w xml:id="d1e392">Xiko</w>
               <w xml:id="d1e394">stiki</w>
               <w xml:id="d1e396">ka'nu</w>
            </seg> - 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e392" xml:lang="en">Sell the big cow.</span>
                   <span target="#d1e392" xml:lang="es">Vende la vaca grande.</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e408" xml:lang="mix">
               <w xml:id="d1e409">sava'a</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e409" xml:lang="en">to cook</span>
                   <span target="#d1e409" xml:lang="es">cocinar</span>
                   <span target="#d1e409" xml:lang="en">to prepare</span>
                   <span target="#d1e409" xml:lang="es">preparar</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e421" xml:lang="mix" type="imperative">
               <w xml:id="d1e422">Sava'a</w>
               <w xml:id="d1e424">chuun</w>
               <w xml:id="d1e426" cert="medium">na</w><!-- modal (maybe should be on verb?) -->
               <w xml:id="d1e428">katso</w>
            </seg> --
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e421" xml:lang="en">Cook the chicken for us to eat</span>
                        <span target="#d1e421" xml:lang="es" cert="medium">Cocina el pollo, para que comamos.</span>
                        
                        <span xml:lang="en" target="#d1e422">cook</span>
                        <span xml:lang="es" target="#d1e422">cocina</span>
                        
                        <span xml:lang="en" target="#d1e426 #d1e428">for us to eat</span>
                        <span xml:lang="es" target="#d1e426 #d1e428">para que comamos</span>
            </spanGrp>
                </hi>
         </p>
         <p>
            <seg xml:id="d1e441" xml:lang="mix">
               <w xml:id="d1e442">saan</w>
            </seg>:
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e442" xml:lang="en">before</span>
                   <span target="#d1e442" xml:lang="es">antes</span>
               </spanGrp>
               <spanGrp type="semantics">
                   <span type="domain" target="#d1e442" ana="#TIME">time</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e454" xml:lang="mix" type="S">
               <w xml:id="d1e455">Saan</w>
               <w xml:id="d1e457">ntsio</w>
               <w xml:id="d1e459">Yucha</w>
               <w xml:id="d1e461">Ncho'o</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e454" xml:lang="en" resp="#TS">Before I lived in Puebla</span>
                        <span xml:lang="en" target="#d1e454" resp="#JB">I used to live in Puebla</span>
                        <span target="#d1e454" xml:lang="es">Antes, yo viviá en Puebla.</span>
                        
                        <span xml:lang="en" target="#d1e455">before</span>
                        <span xml:lang="es" target="#d1e455">antes</span>
                        
                         <span xml:lang="en" target="#d1e457">I lived</span>
                        <span xml:lang="es" target="#d1e457">yo vivía</span>
                        
                        <span xml:lang="en" target="#d1e459 #d1e461">Puebla</span>
                        <span xml:lang="es" target="#d1e459 #d1e461">Puebla</span>
                        
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e477" xml:lang="mix">
               <w xml:id="d1e478">takaa</w><!-- JB: used with "kaa" maybe should be "takaa (U) kaa" -->
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en"/>
                  <span target="#" xml:lang="es"/>
               </spanGrp>like'/‘as' (e.g. “he looks like his father”, “that's the same color as …”</hi>
             
             
         </p>
         <p>
            <seg xml:id="d1e487" xml:lang="mix" type="S">
               <w xml:id="d1e488">Takaa</w>
               <w xml:id="d1e490">xito</w><!-- uncle -->
               <w xml:id="d1e492">ku</w><!-- (poss) 2sg.inf -->
               <w xml:id="d1e494">kau</w><!-- cop (equative) -u (2sg.inf) -->
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e487" xml:lang="en">You look like your uncle.</span>
                        <span target="#d1e487" xml:lang="es">Pareces a tu tío.</span>
                    </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e507" xml:lang="mix">
               <w xml:id="d1e508">vitsi</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e508" xml:lang="en">cold</span>
                   <span target="#d1e508" xml:lang="es">frío</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e517" xml:lang="mix" type="declarative">
               <w xml:id="d1e518">Vitsi</w>
               <w xml:id="d1e520">nchu'a</w>
               <w xml:id="d1e522">yee</w>
               <w xml:id="d1e524">ta</w>
               <w xml:id="d1e526">kuun</w>
               <w xml:id="d1e528">savi</w>
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e517" xml:lang="en">It's very cold when it rains.</span>
                   <span target="#d1e517" xml:lang="es">Hace muy frío quando llueve.</span>
                   
                   <span xml:lang="en" target="#d1e518 #d1e520 #d1e522">it is very cold</span>
                   <span xml:lang="en" target="#d1e518 #d1e520 #d1e522">it's very cold</span>
                   <span xml:lang="es" target="#d1e518 #d1e520 #d1e522">hace muy frío</span>
                   
                   <span xml:lang="en" target="#d1e524">when</span>
                   <span xml:lang="es" target="#d1e524">quando</span>
                   
                   <span xml:lang="en" target="#d1e526 #d1e528">it rains</span>
                   <span xml:lang="en" target="#d1e526 #d1e528">it's raining</span>
                   <span xml:lang="es" target="#d1e526 #d1e528">llueve</span>
                   <span xml:lang="es" target="#d1e526 #d1e528">está llueviendo</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e537" xml:lang="mix">
               <w xml:id="d1e538">kuntsitsi</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">to wear</span>
                  <span target="#" xml:lang="es"/>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e550" xml:lang="mix">
               <w xml:id="d1e551">Kuntsitsi</w>
               <w xml:id="d1e555">sa'ma</w>
               <w xml:id="d1e557">ka'nu</w>
               <w xml:id="d1e559">yo'o</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation"> 
                        <span target="#" xml:lang="en">Wear this big clothes</span>
                        <span target="#" xml:lang="es"/>
                    </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e572" xml:lang="mix">
               <w xml:id="d1e573">ixi</w>
            </seg>:
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e573" xml:lang="en">fur</span>
                        <span target="#d1e573" xml:lang="es">pelo</span>
                        <span target="#d1e573" xml:lang="es">lana</span>
                        <span target="#d1e573" xml:lang="es">pelaje</span>
            </spanGrp>”</hi>
         </p>
         <p>
            <seg xml:id="d1e587" xml:lang="mix" type="declarative" cert="medium"><!-- not "ixi ntikachi ka va'i takua sa'a na sa'ma"? -->
               <w xml:id="d1e588">Ixi</w>
               <w xml:id="d1e590">ntikachi</w>
               <w xml:id="d1e592">ka</w>
               <w xml:id="d1e594">va'i</w><!-- good (-i) -->
               <w xml:id="d1e596">sa'a</w>
                <w xml:id="d1e597">na</w>
               <w xml:id="d1e598">sa'ma</w>
            </seg>
            <hi rend="italic"> -
                    <spanGrp type="translation">
                        <span target="#d1e587" xml:lang="en">The sheep's fur is good to make clothes with.</span>
                        <span target="#d1e587" xml:lang="es">La lana de oveja es bueno para hacer ropas.</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e607" xml:lang="mix" type="MWE">
               <w xml:id="d1e608">sana</w>
               <w xml:id="d1e610">ka'an</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e607" xml:lang="en">goodbye</span>
                   <span target="#d1e607" xml:lang="es">adíos</span>
                   <span target="#d1e607" xml:lang="es">hasta luego</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <hi rend="italic">
               <spanGrp type="usage">
                   <span target="#d1e607" resp="#TS">Not really translated to "good bye" but that what you would say when you are saying goodbye, but it means, we will talk again</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e624" xml:lang="mix">
               <w xml:id="d1e625">tienda</w>
            </seg>:
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e625" xml:lang="en">store</span>
                   <span target="#d1e625" xml:lang="es">tienda</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e634" xml:lang="mix" type="S" cert="medium"><!-- no "takua" or "na" needed? -->
               <w xml:id="d1e635">Nako'on</w>
               <w xml:id="d1e637">tienda</w>
               <pc>,</pc>
               <w xml:id="d1e641">ntakuaan</w><!--  is this just the infinitive? why no "ntakuaan ko" or other inflected form? -->
               <w xml:id="d1e643">mi'ncha</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#" xml:lang="en">Lets go to the store to buy nopales.</span>
                        <span target="#" xml:lang="es">Vamos a la tienda para comprar nopales.</span>
                        
                        <span xml:lang="en" target="#"></span>
                        <span xml:lang="es" target="#"></span>
                                                
                        <span xml:lang="en" target="#d1e643">prickly pear</span>
                        <span xml:lang="es" target="#d1e643">nopal</span>
            </spanGrp>
                <!-- Gram: sentence type ="hortative" -->
                </hi>
         </p>
         <p>
            <seg xml:id="d1e657" xml:lang="mix" resp="#TS" type="compound">
               <w xml:id="d1e658">sata</w>
               <w xml:id="d1e660">tiutsa</w>
            </seg>:
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e657" xml:lang="en">tree bark</span>
                        <span target="#d1e657" xml:lang="en">bark</span>
                        <span target="#d1e657" xml:lang="es">corteza</span>
            </spanGrp></hi>
             
             <seg xml:id="d1e662" xml:lang="mix" resp="#JS" type="compound">
                 <w xml:id="d1e664">sata</w>
                 <w xml:id="d1e666">yutu</w>
             </seg>
             
             <spanGrp type="translation">
                 <span target="#d1e662" xml:lang="en">tree bark</span>
                 <span target="#d1e662" xml:lang="en">bark</span>
                 <span target="#d1e662" xml:lang="es">corteza</span>
             </spanGrp>
         </p>       
         <p>
            <seg xml:id="d1e673" xml:lang="mix" cert="low" type="S"><!-- should this be "nikai"? does he mean past tense  -->
                <w xml:id="d1e674">Nikai</w><!-- (difference between "burn" and "climbed"?) -->
               <w xml:id="d1e678">sata</w><!-- (U) -->
               <w xml:id="d1e680">tiutsa</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e673" xml:lang="en">The tree bark was burning.</span>
                        <span target="#d1e673" xml:lang="es">La corteza estaba quemando.</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e693" xml:lang="mix">
               <w xml:id="d1e694">kua'a</w>
            </seg>
            <hi rend="italic">:
                <spanGrp type="translation">
                    <span target="#d1e694" xml:lang="en">sister</span>
                    <span target="#d1e694" xml:lang="es">hermana</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e703" xml:lang="mix" type="declarative">
               <w xml:id="d1e704">In</w>
               <w xml:id="d1e706">kua'a</w>
               <w xml:id="d1e708">ka</w>
               <w xml:id="d1e710">skua'i</w><!-- is this past or present!? -->
               <w xml:id="d1e712">nu</w>
               <w xml:id="d1e714">universdad</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e703" xml:lang="en">A sister of mine studied at the university.</span>
                        <span target="#d1e703" xml:lang="es">Mi hermana estudió a la universidad.</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e728" xml:lang="mix" type="MWE">
               <w xml:id="d1e729">ntaxito</w>
               <w xml:id="d1e731">tachi</w>
            </seg>:
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e728" xml:lang="en">to breath</span>
                        <span target="#d1e728" xml:lang="es">respirar</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e744" xml:lang="mix" type="imperative">
               <w xml:id="d1e745">Ntaxito</w>
               <w xml:id="d1e747">tachi</w>
               <w xml:id="d1e749">anu</w>
            </seg>, -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e744" xml:lang="en">Breath into your heart.</span>
                        <span target="#d1e744" xml:lang="es">Respira en la corazón.</span>
                        
                        <span xml:lang="en" target="#d1e745 #d1e747">breathe</span>
                        <span xml:lang="es" target="#d1e745 #d1e747">respira</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e762" xml:lang="mix">
               <w xml:id="d1e763">ntakoo</w>
            </seg>
            <hi rend="italic">:
                <spanGrp type="translation">
                    <span target="#d1e763" xml:lang="en">to wake up</span>
                    <span target="#d1e763" xml:lang="es">despertarse</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e772" xml:lang="mix" type="imperative">
               <w xml:id="d1e773">Ntakoo</w>
               <pc>,</pc>
               <w xml:id="d1e779">nako'on</w>
               <w xml:id="d1e783">nu</w>
               <w xml:id="d1e785">yav'i</w>
            </seg>
            <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e772" xml:lang="en">Wake up, lets go to the plaza.</span>
                   <span target="#d1e772" xml:lang="es">Despiértate, vamos a la plaza.</span>
                   
                   <span xml:lang="en" target="#d1e773">wake up</span>
                   <span xml:lang="es" target="#d1e773">despiértate</span>
               </spanGrp>
            </hi>
         </p>
         <list type="unordered">
            <item>
               <hi rend="italic bold">
                  <spanGrp type="usage">
                      <span target="#d1e801" xml:lang="en">(respectful term for ‘uncle'/'aunt' not blood related)</span>
                     <span target="#" xml:lang="es"/>
                  </spanGrp>
               </hi>
            </item>
         </list>
         <p>
            <seg xml:id="d1e800" xml:lang="mix">
               <w xml:id="d1e801">Xito</w>
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">uncle</span>
                  <span target="#" xml:lang="es">tío</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e811" xml:lang="mix">
               <w xml:id="d1e812">Xixi</w>
            </seg> - <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e812" xml:lang="en">aunt</span>
                   <span target="#d1e812" xml:lang="es">tía</span>
               </spanGrp>
            </hi>
            <spanGrp type="usage">
                <span target="#d1e812" resp="#TS">This is how you would refer to them all</span>
            </spanGrp>
         </p>
         <p>
            <seg xml:id="d1e825" xml:lang="mix" type="S">
               <w xml:id="d1e826">Nisa</w>
               <w xml:id="d1e828">kuia</w>
               <w xml:id="d1e830">inkau</w>
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">How old are you?</span>
                  <span target="#" xml:lang="es">¿Quantos años tienes?</span>
                   
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e840" xml:lang="mix" type="MWE">
               <w xml:id="d1e841">kini</w>
               <w xml:id="d1e843">xeen</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e840" xml:lang="en">ugly</span>
                   <span target="#d1e840" xml:lang="es">feo</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e853" xml:lang="mix" type="declarative">
               <w xml:id="d1e854">Kini</w>
               <w xml:id="d1e856">xeen</w>
               <w xml:id="d1e858">kau</w>
               <pc>.</pc>
            </seg> -
                <hi rend="italic" xml:space="preserve"> 
                    <spanGrp type="translation">
                        <span target="#" xml:lang="en">You're ugly.</span>
                        <span target="#" xml:lang="es">Eres feo.</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e874" xml:lang="mix">
               <w xml:id="d1e875">yakua</w>
            </seg>: 
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e875" xml:lang="en">dirty</span>
                   <span target="#d1e875" xml:lang="es">sucio</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e886" xml:lang="mix" type="declarative">
               <w xml:id="d1e887">Yakua</w>
               <w xml:id="d1e889">kau</w>
            </seg> -
                <hi rend="italic" xml:space="preserve">
                    <spanGrp type="translation">
                        <span target="#d1e886" xml:lang="en">you're dirty</span>
                        <span target="#d1e886" xml:lang="es">estas sucio</span>
            </spanGrp></hi>
         </p>
         <p>
            <seg xml:id="d1e903" xml:lang="mix" type="S">
               <w xml:id="d1e904">Nchi</w>
               <w xml:id="d1e906">kuu</w>
               <w xml:id="d1e908">ño'o</w><!-- ñ -->
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                  <span target="#" xml:lang="en">What is this?</span>
                  <span target="#" xml:lang="es">¿Qué es este?</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e918" xml:lang="mix" type="S">
                <pc>¿</pc>
               <w xml:id="d1e919">Nixi</w>
               <w xml:id="d1e921">kunia</w><!-- want (-a) (it) -->
               <w xml:id="d1e923">kachia</w><!-- say (-a) (it)-->
                <pc>?</pc>
            </seg> -
                <hi rend="italic">
               <spanGrp type="translation">
                   <span target="#d1e918" xml:lang="en">What does (X) mean?</span>
                   <span target="#d1e918" xml:lang="es">¿Qué significa (X)?</span>
               </spanGrp>
            </hi>
         </p>
         <p>
            <seg xml:id="d1e933" xml:lang="mix" type="S">
                <pc>¿</pc>
               <w xml:id="d1e934">Nixi</w>
               <w xml:id="d1e936">kachu</w><!-- informal -->
                <pc>?</pc>
            </seg> -
                <hi rend="italic"><spanGrp type="translation">
                    <span target="#d1e933" xml:lang="en">How do you say?</span>
                    <span target="#d1e933" xml:lang="es">Cómo se dice?</span>
               </spanGrp><lb/>
                </hi>
             
             <seg xml:id="d1e943" xml:lang="mix" type="interrogative" resp="#JB" cert="high">
                 <pc>¿</pc>
                 <w xml:id="d1e944">Nixi</w>
                 <w xml:id="d1e946">kachi</w>
                 <w xml:id="d1e948">ni</w><!-- formal -->
                 <pc>?</pc>
             </seg> -
             <hi rend="italic"><spanGrp type="translation">
                 <span target="#d1e943" xml:lang="en">How do you say...?</span>
                 <span target="#d1e943" xml:lang="es">Cómo se dice...?</span>
             </spanGrp><lb/>
             </hi>
         </p>
      </body>
   </text>
</TEI>
