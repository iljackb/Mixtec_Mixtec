  <text>
      <body>
         <entry>
            <form type="lemma">
               <orth>a a a</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">a</def> 
               <usg type="gram">(interiectio admirantis)</usg>
            </sense>
         </entry>
         
         
         <!-- original processing:
           <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">hooca</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">más (comparativo) ; otra vez</def>
            </sense>
         </entry>
         
         -->
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">hooca</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">más</def> <usg type="gram">(comparativo)</usg><pc>;</pc> <def xml:lang="es">otra vez</def>
            </sense>
         </entry>
         
         
         <!-- 
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">huadza</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">en ninguna manera ; en ninguna parte ; ni a una parte ni a otra ;
                  ni en un lugar ni en otro</def>
            </sense>
         </entry>
         -->
         <entry>
            <form type="lemma">
               <orth>huadza</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">en ninguna manera;</def>
               <def xml:lang="es">en ninguna parte;</def> 
               <def xml:lang="es">ni a una parte ni a otra;</def>
               <def xml:lang="es">ni en un lugar ni en otro</def>
            </sense>
         </entry>
         <!-- idzu isa cuiñe, futuro q cuiñe : garañón, caballo -->
         <!-- orig: Garañon cauallo. ydzu ysacuiñe, futuro,
qcuiñe, -->
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">idzu isa cuiñe, futuro q cuiñe</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">garañón, caballo</def>
            </sense>
         </entry>
         
         <!-- modernized spanish in square brackets -->
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">idzi qua ndaa ndeye</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">redropelo</def>
               <cit type="translation">
                  <quote xml:lang="es">[redopelo]</quote>
               </cit>
            </sense>
         </entry>
         
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">idzo dzuchi</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">lebrastilla</def>
               <cit type="translation">
                  <quote xml:lang="es">[lebrastón, lebrato]</quote>
               </cit>
            </sense>
         </entry>
         
         <!-- etymological content -->
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">cate yuvua cate huinda yosasi</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">pobre puesto en grande miseria</def>
            </sense>
            <etym>
               (dicho por metáfora)
            </etym>
         </entry>
         
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">cono mini</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">fenecer, acabar el que muere</def>
            </sense>
            <etym>
               (por metáfora)
            </etym>
         </entry>
         
         <!-- inflected form -->
         <!-- indita dzico, futuro cuindita : ser ensalzado -->
         <entry>
            <form type="lemma">
               <orth>indita dzico</orth><pc>,</pc>
               <form type="inflected">
                  <gramGrp><tns>futuro</tns></gramGrp>
                  <orth>cuindita</orth>
               </form>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">esparcirse el olor</def>
            </sense>
         </entry>
         
         
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">nicuvui dziyo nicuvui nani nisidzota,</orth>
               <form type="inflected">
                  <gramGrp><tns>futuro</tns></gramGrp>
                  <orth>cuidzo</orth>
               </form>
            </form>
            <sense>
               <def xml:lang="es">abarrisco llevar el ladrón todo cuan to hay</def>
            </sense>
         </entry>
