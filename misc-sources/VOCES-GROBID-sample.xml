  <text>
      <body>
         <!-- italics in round brackets are grammatical usage info -->
         <entry>
            <form type="lemma">
               <orth>a a a</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">a</def> 
               <gramGrp><gram>(interiectio admirantis)</gram></gramGrp>
            </sense>
         </entry>
         
         
         
         <!-- original processing:
           <entry>
            <form type="lemma">
               <orth>hooca</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">más (comparativo) ; otra vez</def>
            </sense>
         </entry>
         
         -->
         <entry>
            <form type="lemma">
               <orth>hooca</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">más <gramGrp><gram>(comparativo)</gram></gramGrp>;otra vez</def>
            </sense>
         </entry>
         
         <!-- sa cuvui huaha : poder (nombre) ; posible cosa que puede ser -->
         
         <!--   sa ña : sin (preposición) -->
         
         <!-- THIS WILL MESS UP: 
        
         hmii: a (interiectio del que se queja de alguna enfer-
medad)
         -->
         
         <!-- sa ñeni [sa coo cuita ndicandii] : a la tarde
         the square brackets in left (lemma side) are related
         entries that in the original version were part of a single entry
         -->
         
         <entry>
            <form type="lemma">
               <orth>sa ñeni</orth> 
            </form>
            <xr>[sa coo cuita ndicandii]</xr>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">a la tarde</def>
            </sense>
         </entry>
         
         
         <!-- 
         <entry>
            <form type="lemma">
               <orth>huadza</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">en ninguna manera ; en ninguna parte ; ni a una parte ni a otra ;
                  ni en un lugar ni en otro</def>
            </sense>
         </entry>
         -->
         <entry>
            <form type="lemma">
               <orth>huadza</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">en ninguna manera; en ninguna parte; ni a una parte ni a otra; ni en un lugar ni en otro</def>
            </sense>
         </entry>
         <!-- idzu isa cuiñe, futuro q cuiñe : garañón, caballo -->
         <!-- orig: Garañon cauallo. ydzu ysacuiñe, futuro,
qcuiñe, -->
         <entry>
            <form type="lemma">
               <orth>idzu isa cuiñe, futuro q cuiñe</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">garañón, caballo</def>
            </sense>
         </entry>
         
         <!-- modernized spanish translations in square brackets -->
         <!-- FALSE MATCH IN:
            dzuma dzeque iyo yuhundi nuuta: rost[r]ituerto
enojado estar 
         -->
         
         <entry>
            <form type="lemma">
               <orth>idzi qua ndaa ndeye</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">redropelo</def>
               <cit type="translation">
                  <quote xml:lang="es">[redopelo]</quote>
               </cit>
            </sense>
         </entry>
         
         <entry>
            <form type="lemma">
               <orth>idzo dzuchi</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">lebrastilla</def>
               <cit type="translation">
                  <quote xml:lang="es">[lebrastón, lebrato]</quote>
               </cit>
            </sense>
         </entry>
         
         <!-- etymological content -->
         <entry>
            <form type="lemma">
               <orth>cate yuvua cate huinda yosasi</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">pobre puesto en grande miseria</def>
            </sense>
            <etym>
               (dicho por metáfora)
            </etym>
         </entry>
         
         <entry>
            <form type="lemma">
               <orth>cono mini</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">fenecer, acabar el que muere</def>
            </sense>
            <etym>
               (por metáfora)
            </etym>
         </entry>
         
         <!-- inflected form -->
         <!-- indita dzico, futuro cuindita : ser ensalzado -->
         <entry>
            <form type="lemma">
               <orth>indita dzico</orth><pc>,</pc>
               <form type="inflected">
                  <gramGrp><tns>futuro</tns></gramGrp>
                  <orth>cuindita</orth>
               </form>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">esparcirse el olor</def>
            </sense>
         </entry>
         
         <!-- nicuvui dziyo nicuvui nani nisidzota, futuro
cuidzo : abarrisco llevar el ladrón todo cuan to hay -->
         <entry>
            <form type="lemma">
               <orth>nicuvui dziyo nicuvui nani nisidzota</orth><pc>,</pc>
               <form type="inflected">
                  <gramGrp><tns>futuro</tns></gramGrp>
                  <orth>cuidzo</orth>
               </form>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">abarrisco llevar el ladrón todo cuan to hay</def>
            </sense>
         </entry>
         <!-- original:
            caa dzahui (quachita) : encubierto estar (defecto de otro) -->
         
         
         <!-- sa cuvui coo yee / coo ñaha dzehe : casadero -->
         
         <!-- COMMA BETWEEN BOLD FORMS
         hua dzevui dzo ndaani, hua dzevui dzo cuite- ni: no solamente
         -->
         <entry>
            <form type="lemma">
               <orth xml:lang="nds-x-clmx">hua dzevui dzo ndaani</orth><pc>,</pc>
            </form>
            <form>
               <orth>hua dzevui dzo cuite- ni</orth>
            </form>
            <pc>:</pc>
            <sense>
               <def xml:lang="es">no solamente</def>
            </sense>
         </entry>
         
         <!-- figure out!! -->
         <!-- indita sita ñuhu, ita sita, ita coyo: caer tierra de
arriba, dando de golpes -->
         
         <!-- encode (9 fully representive pages); get from different parts of dictionary -->
      </body>
